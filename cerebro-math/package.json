  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 100,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "devDependencies": {
    "cerebro-scripts": "^0.0.19",
    "rimraf": "^2.6.1"
  }
